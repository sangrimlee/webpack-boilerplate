{
  "name": "webpack-boilerplate",
  "version": "1.0.0",
  "description": "Webpack Boilerplate",
  "keywords": [
    "webpack",
    "express",
    "babel",
    "Boilerplate"
  ],
  "author": "sangrimlee <contact@sangrim.dev>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sangrimlee/webpack-boilerplate.git"
  },
  "bugs": {
    "url": "https://github.com/sangrimlee/webpack-boilerplate/issues"
  },
  "homepage": "https://github.com/sangrimlee/webpack-boilerplate#readme",
  "license": "MIT",
  "main": "src/index.js",
  "scripts": {
    "build": "webpack",
    "build:prod": "NODE_ENV=production webpack",
    "lint": "eslint --fix 'src/**/*.js' 'public/**/*.js'",
    "format": "prettier --write 'src/**/*.js' 'public/**/*.js'",
    "dev": "nodemon --exec babel-node src/index.js",
    "start": "babel-node src/index.js",
    "start:prod": "npm run build:prod && NOD_ENV=production babel-node src/index.js",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@babel/cli": "^7.18.6",
    "@babel/core": "^7.18.6",
    "@babel/node": "^7.18.6",
    "@babel/preset-env": "^7.18.6",
    "babel-loader": "^8.2.5",
    "core-js": "^3.23.4",
    "css-loader": "^6.7.1",
    "css-minimizer-webpack-plugin": "^4.0.0",
    "eslint": "^8.19.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^8.5.0",
    "express": "^4.18.1",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^8.0.0",
    "lint-staged": "^13.0.3",
    "mini-css-extract-plugin": "^2.6.1",
    "nodemon": "^2.0.19",
    "prettier": "^2.7.1",
    "terser-webpack-plugin": "^5.3.3",
    "url-loader": "^4.1.1",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-middleware": "^5.3.3"
  }
}
